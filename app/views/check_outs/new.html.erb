<h2>Check Out Book</h2>

<%= form_for @check_out do |f| %>

	<div class="field">
		<%= f.label :user_id, "User Id" %>
  	<%= f.text_field :user_id %>
  </div>

	<div class="field">
  	<%= f.label :book_copy_id, "Book Copy Id" %>
  	<%= f.text_field :book_copy_id %>
  </div>

  <div class="action"><%= f.submit "Check Out Book" %></div>

<% end %>



